<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<!-- <select id="postView" parameterType="map" resultType="list"> -->
<mapper namespace="com.example.demo.mapper.LogSearchMapper">
	<select id="logSearch" resultType="com.example.demo.model.LogDTO" parameterType="String">
		select *
		
		from market.login_log
		where 
			id = #{id}
	  	 
	</select>
	
</mapper>

