<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<!-- <select id="postView" parameterType="map" resultType="list"> -->
<mapper namespace="com.example.demo.mapper.ForgetPwMapper">
	<select id="forgetPw" resultType="com.example.demo.model.MemberDTO" parameterType="map">
	
		select *
		from market.member_info02 
		where 
			mem_email = #{mem_email}
			and
			mem_id = #{mem_id}
	  	 
	  	 
	</select>
	
</mapper>